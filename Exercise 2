public class DistCalc {

	public static void main(String[] args) {
		int x1 = 1;
		int y1 = 2;
		int x2 = 5;
		int y2 = 3;
		
		double distance;
		
		distance = Math.sqrt((x2 - x1)*(x2 - x1) + (y2 - y1)*(y2 - y1));
		
		System.out.println("The distance between the two points is: " +distance);

	}

}
